# Database
DATABASE_URL="postgresql://username:password@localhost:5432/aima_db"

# JWT
JWT_SECRET="your-super-secret-jwt-key-here"
JWT_EXPIRES_IN="7d"

# Server
PORT=3001
NODE_ENV="development"

# CORS
CORS_ORIGIN="http://localhost:5173"

# File Upload
MAX_FILE_SIZE=100000000
UPLOAD_DIR="./uploads"

# GPU Provider API Keys
RUNPOD_API_KEY="your-runpod-api-key-here"
VASTAI_API_KEY="your-vastai-api-key-here"

# GPU Provider Settings
GPU_PROVIDER_TIMEOUT=30000
GPU_INSTANCE_CHECK_INTERVAL=60000
BATCH_PROCESSING_MAX_CONCURRENT_JOBS=10
BATCH_PROCESSING_QUEUE_SIZE=100

# Cost Tracking
COST_TRACKING_ENABLED=true
COST_ALERT_THRESHOLD=100.00

# Monitoring
METRICS_COLLECTION_INTERVAL=30000
METRICS_RETENTION_DAYS=30